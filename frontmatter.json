{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "posts",
      "previewPath": "'posts'",
      "pageBundle": true,
      "clearEmpty": false,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "pubDate",
          "name": "pubDate",
          "type": "datetime"
        },
        {
          "title": "modDate",
          "name": "modDate",
          "type": "datetime"
        },
        {
          "title": "location",
          "name": "location",
          "type": "fields",
          "fields": [
            {
              "title": "coordinates",
              "name": "coordinates",
              "type": "string"
            },
            {
              "title": "alias",
              "name": "alias",
              "type": "string"
            }
          ]
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "image",
          "name": "image",
          "type": "fields",
          "fields": [
            {
              "title": "url",
              "name": "url",
              "type": "string"
            },
            {
              "title": "alt",
              "name": "alt",
              "type": "string"
            }
          ]
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "pageEmoji",
          "name": "pageEmoji",
          "type": "string"
        },
        {
          "title": "fmContentType",
          "name": "fmContentType",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/src/content/posts",
      "contentTypes": [
        "posts"
      ]
    }
  ],
  "frontMatter.content.publicFolder": "public",
  "frontMatter.git.enabled": true
}